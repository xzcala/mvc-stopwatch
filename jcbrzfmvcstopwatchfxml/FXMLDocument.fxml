<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.scene.image.*?>
<?import javafx.geometry.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox prefHeight="420.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="jcbrzfmvcstopwatchfxml.FXMLDocumentController">
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
   <children>
      <StackPane prefHeight="215.0" prefWidth="200.0">
         <VBox.margin>
            <Insets top="20.0" />
         </VBox.margin>
         <children>
            <ImageView fitHeight="215.0" fitWidth="325.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@clockface.png" />
               </image>
            </ImageView>
            <ImageView fx:id="hand" fitHeight="200.0" fitWidth="200.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@hand.png" />
               </image></ImageView>
         </children>
      </StackPane>
      <VBox prefHeight="110.0" prefWidth="100.0">
         <children>
            <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0">
               <children>
                  <Text fx:id="clockLabel" strokeType="OUTSIDE" strokeWidth="0.0" text="00:00.0">
                     <font>
                        <Font size="24.0" />
                     </font>
                  </Text>
               </children>
            </HBox>
            <TilePane alignment="CENTER" hgap="30.0" prefHeight="200.0" prefWidth="200.0">
               <children>
                  <Button fx:id="startStopButton" mnemonicParsing="false" onAction="#startStopTimer" prefWidth="100.0" text="Start" />
                  <Button fx:id="recordResetButton" mnemonicParsing="false" onAction="#recordResetTimer" prefWidth="100.0" text="Record" />
               </children>
            </TilePane>
            <VBox alignment="CENTER" prefHeight="200.0" prefWidth="100.0">
               <children>
                  <Text fx:id="display1" strokeType="OUTSIDE" strokeWidth="0.0" text="--:--.s" textAlignment="CENTER" wrappingWidth="300.0">
                     <font>
                        <Font size="20.0" />
                     </font>
                  </Text>
                  <Text fx:id="display2" layoutX="162.0" layoutY="38.0" strokeType="OUTSIDE" strokeWidth="0.0" text="--:--.s" textAlignment="CENTER" wrappingWidth="300.0">
                     <font>
                        <Font size="20.0" />
                     </font>
                  </Text>
                  <Text fx:id="display3" layoutX="162.0" layoutY="59.0" strokeType="OUTSIDE" strokeWidth="0.0" text="--:--.s" textAlignment="CENTER" wrappingWidth="300.0">
                     <font>
                        <Font size="20.0" />
                     </font>
                  </Text>
               </children>
            </VBox>
         </children></VBox>
   </children>
</VBox>
